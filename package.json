{
  "name": "postcss-plugins",
  "version": "1.12.4",
  "description": "A consolidated list of PostCSS plugins in an ready-to-use format.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/himynameisdave/postcss-plugins.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "add": "node scripts/addNewPlugin.js && node scripts/updateAuthors.js",
    "add-new-plugin": "node scripts/addNewPlugin.js",
    "update-authors": "node scripts/updateAuthors.js",
    "update-stars": "node scripts/updateStars.js && node scripts/updateAuthors.js",
    "precommit": "node scripts/updateAuthors.js && npm run lint && npm run test",
    "lint": "eslint ./scripts/ ./test/",
    "test": "mocha test"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "inquirer": "^3.3.0",
    "octonode": "^0.9.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "chai": "^4.1.2",
    "chai-json-schema": "^1.5.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "fs-extra": "^4.0.2",
    "husky": "^0.14.3",
    "mocha": "^4.0.1"
  },
  "keywords": [
    "postcss",
    "postcss-plugin",
    "css",
    "list",
    "library",
    "plugins"
  ],
  "homepage": "https://github.com/himynameisdave/postcss-plugins#readme",
  "bugs": {
    "url": "https://github.com/himynameisdave/postcss-plugins/issues"
  },
  "author": "himynameisdave",
  "license": "MIT"
}
